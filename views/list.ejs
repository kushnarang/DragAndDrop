<html>
	<head>
		<script src="https://taxi.house/jquery331.js"></script>
		<title>Drag And Drop</title>
		<style>
			@import url('https://fonts.googleapis.com/css?family=Noto+Sans|Material+Icons');

			body
			{
				display: flex;
				flex-direction: column;
				align-items: center;
				justify-content: center;
			}
			.list
			{
				width: 100%;
				min-height: 50px;
				padding: 9px 15px;
				box-sizing: border-box;
				font-family: 'Noto Sans', sans-serif;
				display: flex;
				flex-direction: row;
				justify-content: space-between;
				align-items: center;
				border-bottom: 1px solid rgba(0, 0, 0, 0.15);
			}
			.list i
			{
				font-size: 150%;
			}
			.list:first-of-type
			{
				border-top: 1px solid rgba(0, 0, 0, 0.15);
			}
		</style>
	</head>
	<body>

	</body>
	<script>
		$(document).ready(() =>
		{
			$.ajax({ method: "POST", url: "get"}).done((dat, err) =>
			{
				console.log(dat);
				dat.forEach(el =>
				{
					let split = el.split("\\");
					let splitEnd = split[split.length - 1];
					console.log(splitEnd);
					$("body").append(`
						<div class="list">
							${splitEnd}
							<a download="${splitEnd}" href="/files/${split[split.length - 2]}"><i class="material-icons">save</i></a>
						</div>
					`);
				});
			});
		});
	</script>
</html>